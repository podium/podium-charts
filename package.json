{
  "name": "podium-charts",
  "version": "0.2.3",
  "description": "A collection of Podium chart components",
  "main": "build/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "start-storybook -p 9001 -c .storybook --quiet",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css}\"",
    "lint": "eslint src/** --fix --ext .jsx,.js,.ts,.tsx .",
    "build": "babel src --out-dir build --presets=@babel/preset-env,@babel/preset-react --plugins=transform-class-properties,lodash"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css}": [
      "prettier --write",
      "eslint --fix --config react-app --max-warnings 0",
      "git add"
    ]
  },
  "author": "Some Notion",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@podiumhq/podium-ui": "^12.1.2",
    "@storybook/addon-info": "^4.0.11",
    "@storybook/react": "^4.0.10",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "eslint": "^5.9.0",
    "eslint-config-react-app": "^3.0.5",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.2.0",
    "lint-staged": "^4.2.3",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "prettier": "^1.15.3",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "styled-components": "^4.1.2"
  },
  "dependencies": {
    "humanize-duration": "^3.16.0",
    "recharts": "^1.4.1"
  },
  "peerDependencies": {
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "@podiumhq/podium-ui": "^12.1.2",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "styled-components": "^4.1.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none"
  }
}
